// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Biblioteca {
  id Int @id @default(autoincrement())
  nombreD String @unique
  imgLink String @unique
  descricion String 
  docLink String @unique
}

model User{
  id Int @id @default(autoincrement())
  nombre String
  correo String @unique
  password String
}

model Home{
  id Int @id @default(autoincrement())
  titulo String
  urlImagen String
  contenido String
}